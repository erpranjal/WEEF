<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.weef.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_programme"
        android:name="com.example.weef.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/programme" />

    <fragment
        android:id="@+id/navigation_general"
        android:name="com.example.weef.ui.notifications.NotificationsFragment"
        android:label="@string/title_General"
        tools:layout="@layout/general" />

    <fragment
        android:id="@+id/navigation_contact"
        android:name="com.example.weef.ui.contact.ContactFragment"
        android:label="@string/title_contact"
        tools:layout="@layout/contact" />

    <fragment
        android:id="@+id/navigation_sponsor"
        android:name="com.example.weef.ui.notifications.NotificationsFragment"
        android:label="@string/title_sponsor"
        tools:layout="@layout/sponsor" />
</navigation>